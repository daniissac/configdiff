{
  "hostname": "core-rtr-01",
  "domain": "network.example.com",
  "interfaces": {
    "GigabitEthernet0/0": {
      "description": "Uplink to ISP",
      "ip_address": "203.0.113.1",
      "subnet_mask": "255.255.255.252",
      "enabled": true,
      "mtu": 1500,
      "speed": "auto",
      "duplex": "auto"
    },
    "GigabitEthernet0/1": {
      "description": "LAN Interface",
      "ip_address": "10.0.0.1",
      "subnet_mask": "255.255.255.0",
      "enabled": true,
      "mtu": 9000,
      "vlan": 100
    },
    "Loopback0": {
      "ip_address": "192.168.255.1",
      "subnet_mask": "255.255.255.255"
    }
  },
  "routing": {
    "ospf": {
      "process_id": 1,
      "router_id": "192.168.255.1",
      "areas": [
        {
          "id": "0.0.0.0",
          "networks": ["10.0.0.0/24", "192.168.255.1/32"]
        }
      ]
    },
    "static_routes": [
      {"network": "0.0.0.0/0", "next_hop": "203.0.113.2"}
    ]
  },
  "dns": {
    "servers": ["10.0.0.2", "10.0.0.3"],
    "domain_lookup": true
  },
  "ntp": {
    "servers": ["10.0.0.10"],
    "timezone": "UTC"
  },
  "acl": [
    {
      "name": "MGMT-ACCESS",
      "entries": [
        {"sequence": 10, "action": "permit", "source": "10.0.0.0/24"},
        {"sequence": 20, "action": "permit", "source": "172.16.0.0/12"},
        {"sequence": 100, "action": "deny", "source": "any"}
      ]
    }
  ]
}
